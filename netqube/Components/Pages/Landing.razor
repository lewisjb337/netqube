@using Sidio.Sitemap.Blazor
@page "/"

@attribute [Sitemap]

@rendermode InteractiveServer

<PageTitle>Netqube - Home</PageTitle>

<div class="hero-wrapper">
    <div class="container position-relative z-2 pb-5">
        <div class="row justify-content-center pt-3 hero-content">
            <div class="col-lg-9 col-xl-8 text-center pt-5 mt-1">
                <h1 class="display-3 pt-3 mt-3 mb-4">Back-End Solutions. <br/> Built to Scale.</h1>
                <p class="opacity-70 fs-5">
                    We design powerful APIs, cloud infrastructure, and data systems that evolve with your business — 
                    secure, efficient, and future-ready.
                </p>

                <div class="d-flex justify-content-center gap-3 mt-5">
                    <a href="#contact" class="custom-button-large px-4">
                        <i class="fa-regular fa-message" style="margin-right: 10px;"></i>Get In Touch
                    </a>
                    <a href="#services" class="custom-button-outline-large px-4">
                        <i class="fa-solid fa-circle-info" style="margin-right: 10px;"></i>Learn More
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>

<section class="container pb-2 pb-sm-3 pb-md-4 pb-lg-5 hero-subsection-wrapper">
    <div class="card hero-subsection">
        <div class="row row-cols-1 row-cols-md-3 g-0 text-center">
            <div class="col d-md-flex justify-content-center position-relative">
                <div class="position-absolute top-50 end-0 translate-middle-y border-end statistics-divider d-none d-md-block"></div>
                <div class="p-4 px-sm-0 py-sm-5">
                    <div class="h2 display-5 mb-0 text-green">1-on-1</div>
                    <div class="text-blue fw-bold">Technical Project Onboarding</div>
                    <small class="text-muted">Personalized guidance from kickoff to deployment.</small>
                </div>
            </div>
            <div class="col d-md-flex justify-content-center position-relative">
                <div class="p-4 px-sm-0 py-sm-5">
                    <div class="h2 display-5 mb-0 text-green">Client Portal</div>
                    <div class="text-blue fw-bold">Access from Day One</div>
                    <small class="text-muted">Track progress, request updates, view documents easily.</small>
                </div>
            </div>
            <div class="col d-md-flex justify-content-center position-relative">
                <div class="position-absolute top-50 end-0 translate-middle-y border-end statistics-divider d-none d-md-block"></div>
                <div class="p-4 px-sm-0 py-sm-5">
                    <div class="h2 display-5 mb-0 text-green">ISO-aligned</div>
                    <div class="text-blue fw-bold">Security Practices</div>
                    <small class="text-muted">Modern, standards-driven infrastructure by default.</small>
                </div>
            </div>
        </div>
    </div>
</section>

